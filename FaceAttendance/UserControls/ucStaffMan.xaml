                                            Content="Download All Staff"
                                            FontSize="14"
                                            Style="{StaticResource 3DButtonStyle}"
                                            TabIndex="6" />
                                        <Grid
                                            Width="30"
                                            Height="30"
                                            HorizontalAlignment="Right"
                                            MouseDown="down_rel_list">
                                            <Image
                                                Name="downUser"
                                                Grid.Column="2"
                                                Width="30"
                                                Height="30"
                                                Margin="0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center">
                                                <Image.Source>
                                                    <BitmapImage UriSource="..\Images\refresh.png" />
                                                </Image.Source>
                                            </Image>
                                        </Grid>
                                    </StackPanel>





                                    <Image
                                        Name="D_Wait"
                                        Grid.Column="1"
                                        Width="50"
                                        Height="30"
                                        Margin="0,0,150,0"
                                        HorizontalAlignment="Left"
                                        VerticalAlignment="Center"
                                        Visibility="Collapsed">
                                        <Image.Source>
                                            <BitmapImage UriSource="..\Images\waitsand.png" />
                                        </Image.Source>
                                    </Image>
                                </Grid>
                            </StackPanel>









                            <StackPanel MaxHeight="380" Margin="0,0,0,0">
                                <Border
                                    Height="336"
                                    BorderBrush="{StaticResource VISION}"
                                    BorderThickness="2"
                                    CornerRadius="1"
                                    DockPanel.Dock="Top">


                                    <StackPanel
                                        Height="336"
                                        Margin="0,0,0,0"
                                        HorizontalAlignment="Stretch">
                                        <!--  This is the hidden helper Grid which does the resizing  -->
                                        <Grid Visibility="Hidden">
                                            <Grid.ColumnDefinitions>

                                                <ColumnDefinition Width="0.65*" />
                                                <ColumnDefinition Width="1*" />
                                                <ColumnDefinition Width="1*" />
                                                <ColumnDefinition Width="1*" />
                                                <ColumnDefinition Width="1*" />
                                                <ColumnDefinition Width="1*" />
                                                <ColumnDefinition Width="1*" />
                                            </Grid.ColumnDefinitions>
                                            <!--  This is the hidden helper Field which is used to bind to, using the "Fill" column of the helper grid  -->
                                            <Grid x:Name="c0" Grid.Column="0" />
                                            <Grid x:Name="c1" Grid.Column="1" />
                                            <Grid x:Name="c2" Grid.Column="2" />
                                            <Grid x:Name="c3" Grid.Column="3" />
                                            <Grid x:Name="c4" Grid.Column="4" />
                                            <Grid x:Name="c5" Grid.Column="5" />
                                            <Grid x:Name="c6" Grid.Column="6" />
                                            <Grid x:Name="c7" Grid.Column="6" />
                                        </Grid>

                                        <ListView
                                            x:Name="downuserlist"
                                            Height="336"
                                            MaxHeight="336"
                                            Margin="0,0,4,0"
                                            VerticalAlignment="Top"
                                            HorizontalContentAlignment="Center"
                                            VerticalContentAlignment="Top"
                                            Background="Transparent"
                                            FontSize="22"
                                            Foreground="Black"
                                            ItemsSource="{Binding Udown_Disp}"
                                            ScrollViewer.CanContentScroll="True"
                                            ScrollViewer.HorizontalScrollBarVisibility="Visible"
                                            ScrollViewer.VerticalScrollBarVisibility="Visible">

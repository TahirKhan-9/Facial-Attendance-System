                                                                            FontSize="12" />
                                                                    </InlineUIContainer></TextBlock>
                                                            </DataTemplate>
                                                        </GridViewColumn.CellTemplate>
                                                    </GridViewColumn>

                                                    <GridViewColumn Width="{Binding ActualWidth, ElementName=c2}" Header="User ID">
                                                        <GridViewColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <TextBlock
                                                                    Padding="0"
                                                                    HorizontalAlignment="Left"
                                                                    VerticalAlignment="Stretch"><InlineUIContainer>

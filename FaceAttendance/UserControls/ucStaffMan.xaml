                                    <Setter Property="Background" Value="{StaticResource HIK}" />
                                </Trigger>
                            </Style.Triggers>
                        </Style>

                    </ListBox.ItemContainerStyle>
                    <ListView.Resources>
                        <Style TargetType="ListViewItem">
                            <Setter Property="Height" Value="0" />

                        </Style>
                    </ListView.Resources>
                    <ListView.View>

                        <GridView>
                            <GridView.ColumnHeaderContainerStyle>
                                <Style TargetType="{x:Type GridViewColumnHeader}">
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="{x:Type GridViewColumnHeader}">
                                                <Border
                                                    Background="Transparent"
                                                    BorderBrush="Black"
                                                    BorderThickness="0,0,0,0">
                                                    <TextBlock
                                                        x:Name="ContentHeader"
                                                        Width="{TemplateBinding Width}"
                                                        Padding="0,5,5,5"
                                                        Text="{TemplateBinding Content}"
                                                        TextAlignment="Left" />
                                                </Border>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                    <Setter Property="OverridesDefaultStyle" Value="True" />
                                    <Setter Property="Foreground" Value="{StaticResource FREE}" />
                                    <Setter Property="FontFamily" Value="Champagne &amp; Limousines" />
                                    <Setter Property="FontSize" Value="12" />
                                    <Setter Property="FontWeight" Value="Bold" />
                                    <Setter Property="Background" Value="AliceBlue" />
                                    <Setter Property="BorderBrush" Value="BlanchedAlmond" />
                                    <Setter Property="BorderThickness" Value="2" />

                                </Style>
                            </GridView.ColumnHeaderContainerStyle>
                            <GridViewColumn Width="{Binding ElementName=c1, Path=ActualWidth}" Header="Group ID">
                                <GridViewColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock
                                            Padding="0"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch">
                                            <Label
                                                Padding="0"
                                                Content="{Binding ID}"
                                                FontSize="12" />
                                        </TextBlock>
                                    </DataTemplate>
                                </GridViewColumn.CellTemplate>
                            </GridViewColumn>
                            <GridViewColumn Width="{Binding ElementName=c2, Path=ActualWidth}" Header="Group Name">
                                <GridViewColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock
                                            Padding="0"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch">
                                            <Label
                                                Padding="0"
                                                Content="{Binding Group_Name}"
                                                FontSize="12" />
                                        </TextBlock>
                                    </DataTemplate>
                                </GridViewColumn.CellTemplate>
                            </GridViewColumn>
                            <GridViewColumn
                                x:Name="action1"
                                Width="{Binding ElementName=c6, Path=ActualWidth}"
                                Header="Action">
                                <GridViewColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock
                                            Padding="0"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Stretch">
                                            <TextBlock
                                                Name="abc"
                                                Width="65"
                                                Cursor="Hand"
                                                Focusable="True">
                                                <CheckBox IsChecked="{Binding RelativeSource={RelativeSource AncestorType={x:Type ListBoxItem}}, Path=IsSelected}" />
                                                <TextBlock Margin="0,2,2,0" HorizontalAlignment="Center">
                                                    <Label
                                                        Padding="0,0,0,3"
                                                        FontFamily="Champagne &amp; Limousines"
                                                        FontSize="11"
                                                        Foreground="{StaticResource VISION}" />
                                                </TextBlock>
                                            </TextBlock>

                                        </TextBlock>

                                    </DataTemplate>
                                </GridViewColumn.CellTemplate>
                            </GridViewColumn>

                        </GridView>
                    </ListView.View>

                </ListView>

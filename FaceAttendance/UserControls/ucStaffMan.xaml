                                        <TransformGroup>
                                            <ScaleTransform />
                                            <SkewTransform AngleX="-0.14" />
                                            <RotateTransform />
                                            <TranslateTransform X="-0.331" />
                                        </TransformGroup>
                                    </Border.RenderTransform>
                                    <StackPanel
                                        Grid.Column="1"
                                        Height="150"
                                        VerticalAlignment="Center"
                                        Orientation="Vertical">



                                        <Image
                                            Name="Arrowbb"
                                            Width="50"
                                            Height="30"
                                            Margin="0,30,0,0"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Bottom"
                                            MouseDown="UnBlockuserMaster">
                                            <Image.Source>
                                                <BitmapImage UriSource="..\Images\unblock_user.png" />
                                            </Image.Source>

                                        </Image>


                                        <Image
                                            Name="doubleArrowfb"
                                            Width="50"
                                            Height="30"
                                            Margin="0,30,0,0"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Bottom"
                                            MouseDown="BlockUserMaster">
                                            <Image.Source>
                                                <BitmapImage UriSource="..\Images\block_user.png" />
                                            </Image.Source>
                                        </Image>
                                        <!--<materialDesign:PackIcon
                                            Margin="0,30,0,0"
                                Height="30"
                                Width="50"
                                x:Name="doubleArrowf"
                            HorizontalAlignment="Center"
                            VerticalAlignment="Bottom"
                            Foreground="{StaticResource HIK}"
                            Kind="ReplyAll" MouseDown="MoveAllBack"   />-->





                                    </StackPanel>
                                </Border>
                            </Grid>

                            <StackPanel Grid.Column="2">
                                <Border
                                    Height="auto"
                                    BorderBrush="{StaticResource VISION}"
                                    BorderThickness="1"
                                    CornerRadius="5"
                                    RenderTransformOrigin="0.5,0.5">



                                    <StackPanel
                                        Height="336"
                                        Margin="0,0,0,0"
                                        HorizontalAlignment="Stretch">
                                        <!--  This is the hidden helper Grid which does the resizing  -->
                                        <Grid Visibility="Hidden">
                                            <Grid.ColumnDefinitions>

                                                <ColumnDefinition Width="0.65*" />
                                                <ColumnDefinition Width="1*" />
                                                <ColumnDefinition Width="1*" />
                                                <ColumnDefinition Width="1.1*" />
                                                <ColumnDefinition Width="1*" />
                                                <ColumnDefinition Width="1*" />
                                                <ColumnDefinition Width="1*" />
                                            </Grid.ColumnDefinitions>
                                            <!--  This is the hidden helper Field which is used to bind to, using the "Fill" column of the helper grid  -->
                                            <Grid x:Name="rc0b" Grid.Column="0" />
                                            <Grid x:Name="rc1b" Grid.Column="1" />
                                            <Grid x:Name="rc2b" Grid.Column="2" />
                                            <Grid x:Name="rc3b" Grid.Column="3" />
                                            <Grid x:Name="rc4b" Grid.Column="4" />
                                            <Grid x:Name="rc5b" Grid.Column="5" />
                                            <Grid x:Name="rc6b" Grid.Column="6" />
                                            <Grid x:Name="rc7b" Grid.Column="7" />
                                        </Grid>

                                        <ListView
                                            x:Name="buserlist"
                                            Height="336"
                                            MaxHeight="336"
                                            Margin="0,0,4,0"
                                            VerticalAlignment="Top"
                                            VerticalContentAlignment="Top"
                                            Background="Transparent"
                                            FontSize="20"
                                            Foreground="Black"
                                            ItemsSource="{Binding Udown_Disp}"
                                            MouseDown="buserlist_MouseDown"
                                            ScrollViewer.CanContentScroll="True"
                                            ScrollViewer.HorizontalScrollBarVisibility="Visible"
                                            ScrollViewer.VerticalScrollBarVisibility="Visible"
                                            SelectionMode="Multiple">
                                            <ListView.Resources>
                                                <Style TargetType="{x:Type ListViewItem}">
                                                    <Setter Property="Height" Value="0" />

                                                </Style>
                                            </ListView.Resources>

                                            <ListView.ItemContainerStyle>
                                                <Style TargetType="{x:Type ListViewItem}">
                                                    <Style.Triggers>
                                                        <Trigger Property="IsMouseOver" Value="True">
                                                            <Setter Property="Background" Value="{StaticResource HIK}" />
                                                            <Setter Property="Focusable" Value="True" />

                                                        </Trigger>
                                                        <Trigger Property="IsSelected" Value="True">
                                                            <Setter Property="Background" Value="{StaticResource HIK}" />
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>


                                            </ListView.ItemContainerStyle>

                                            <ListView.View>

                                                <GridView>
                                                    <GridView.ColumnHeaderContainerStyle>
                                                        <Style TargetType="{x:Type GridViewColumnHeader}">
                                                            <Setter Property="Template">
                                                                <Setter.Value>
                                                                    <ControlTemplate TargetType="{x:Type GridViewColumnHeader}">
                                                                        <Border
                                                                            Background="Transparent"
                                                                            BorderBrush="Black"
                                                                            BorderThickness="0,0,0,0">
                                                                            <TextBlock
                                                                                x:Name="ContentHeader"
                                                                                Width="{TemplateBinding Width}"
                                                                                Padding="0,5,5,5"
                                                                                Text="{TemplateBinding Content}"
                                                                                TextAlignment="Left" />
                                                                        </Border>
                                                                    </ControlTemplate>
                                                                </Setter.Value>
                                                            </Setter>
                                                            <Setter Property="OverridesDefaultStyle" Value="True" />
                                                            <Setter Property="Foreground" Value="{StaticResource FREE}" />
                                                            <Setter Property="FontFamily" Value="Champagne &amp; Limousines" />
                                                            <Setter Property="FontSize" Value="12" />
                                                            <Setter Property="FontWeight" Value="Bold" />
                                                            <Setter Property="Background" Value="AliceBlue" />
                                                            <Setter Property="BorderBrush" Value="BlanchedAlmond" />
                                                            <Setter Property="BorderThickness" Value="2" />

                                                        </Style>
                                                    </GridView.ColumnHeaderContainerStyle>

                                                    <GridViewColumn Width="{Binding ActualWidth, ElementName=rc0b}" Header="Sr">
                                                        <GridViewColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <TextBlock
                                                                    Padding="0"
                                                                    HorizontalAlignment="Stretch"
                                                                    VerticalAlignment="Stretch"><InlineUIContainer>
                                                                        <Label
                                                                            Padding="0"
                                                                            Content="{Binding SrNo}"
                                                                            FontSize="12" />
                                                                    </InlineUIContainer></TextBlock>
                                                            </DataTemplate>
                                                        </GridViewColumn.CellTemplate>
                                                    </GridViewColumn>

                                                    <GridViewColumn Width="{Binding ActualWidth, ElementName=rc1b}" Header="Device ID">
                                                        <GridViewColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <TextBlock
                                                                    Padding="0"
                                                                    HorizontalAlignment="Stretch"
                                                                    VerticalAlignment="Stretch"><InlineUIContainer>
                                                                        <Label
                                                                            Padding="0"
                                                                            Content="{Binding EMachineNumber}"
                                                                            FontSize="12" />
                                                                    </InlineUIContainer></TextBlock>
                                                            </DataTemplate>
                                                        </GridViewColumn.CellTemplate>
                                                    </GridViewColumn>

                                                    <GridViewColumn Width="{Binding ActualWidth, ElementName=rc2b}" Header="User ID">
                                                        <GridViewColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <TextBlock
                                                                    Padding="0"
                                                                    HorizontalAlignment="Stretch"
                                                                    VerticalAlignment="Stretch"><InlineUIContainer>
                                                                        <Label
                                                                            Padding="0"
                                                                            Content="{Binding EnrollNumber}"
                                                                            FontSize="12" />
                                                                    </InlineUIContainer></TextBlock>
                                                            </DataTemplate>
                                                        </GridViewColumn.CellTemplate>
                                                    </GridViewColumn>

                                                    <GridViewColumn Width="{Binding ActualWidth, ElementName=rc3b}" Header="UserName">
                                                        <GridViewColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <TextBlock
                                                                    Padding="0"
                                                                    HorizontalAlignment="Stretch"
                                                                    VerticalAlignment="Stretch"><InlineUIContainer>
                                                                        <Label
                                                                            Padding="0"
                                                                            Content="{Binding EName}"
                                                                            FontSize="12" />
                                                                    </InlineUIContainer></TextBlock>
                                                            </DataTemplate>
                                                        </GridViewColumn.CellTemplate>
                                                    </GridViewColumn>

                                                    <!--<GridViewColumn Width="{Binding ActualWidth, ElementName=rc4}" Header="Name">
                                                        <GridViewColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <TextBlock
                                            Padding="0"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch"><InlineUIContainer>
                                                                            <Label
                                                                                Padding="0"
                                                                                Content="{Binding EnrollNumber}"
                                                                                FontSize="12" />
                                                                        </InlineUIContainer></TextBlock>
                                                            </DataTemplate>
                                                        </GridViewColumn.CellTemplate>
                                                    </GridViewColumn>-->

                                                    <GridViewColumn
                                                        x:Name="face_rb"
                                                        Width="{Binding ActualWidth, ElementName=rc5b}"
                                                        Header="Type">
                                                        <GridViewColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <TextBlock
                                                                    Padding="0"
                                                                    HorizontalAlignment="Stretch"
                                                                    VerticalAlignment="Stretch"><InlineUIContainer>
                                                                        <Label
                                                                            Padding="0"
                                                                            Content="{Binding type}"
                                                                            FontSize="12" />
                                                                    </InlineUIContainer></TextBlock>
                                                            </DataTemplate>
                                                        </GridViewColumn.CellTemplate>
                                                    </GridViewColumn>

                                                    <GridViewColumn Width="{Binding ActualWidth, ElementName=rc6b}" Header="Privilige">
                                                        <GridViewColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <TextBlock
                                                                    Padding="0"
                                                                    HorizontalAlignment="Stretch"
                                                                    VerticalAlignment="Stretch"><InlineUIContainer>
                                                                        <Label
                                                                            Padding="0"
                                                                            Content="{Binding Privilige}"
                                                                            FontSize="12" />
                                                                    </InlineUIContainer></TextBlock>
                                                            </DataTemplate>
                                                        </GridViewColumn.CellTemplate>
                                                    </GridViewColumn>

                                                    <GridViewColumn Width="{Binding ActualWidth, ElementName=rc7b}" Header="Password">
                                                        <GridViewColumn.CellTemplate>
                                                            <DataTemplate>
                                                                <TextBlock
                                                                    Padding="0"
                                                                    HorizontalAlignment="Stretch"
                                                                    VerticalAlignment="Stretch"><InlineUIContainer>
                                                                        <Label
                                                                            Padding="0"
                                                                            Content="{Binding enPassword}"
                                                                            FontSize="12" />
                                                                    </InlineUIContainer></TextBlock>
                                                            </DataTemplate>
                                                        </GridViewColumn.CellTemplate>
                                                    </GridViewColumn>



                                                </GridView>
                                            </ListView.View>

                                        </ListView>
                                    </StackPanel>

                                </Border>

                            </StackPanel>



                        </Grid>



                    </Grid>
                </TabItem>
            </TabControl>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*" />
                    <ColumnDefinition Width="1*" />
                </Grid.ColumnDefinitions>
                <TextBlock
                    Grid.Column="0"
                    Margin="0,00,0,0"
                    HorizontalAlignment="Left">
                    <Label
                        Name="lbldevStatus"
                        Content="StatusWindow"
                        FontSize="20"
                        Foreground="#417AAB" />
                </TextBlock>
                <StackPanel
                    x:Name="usersCount"
                    Grid.Column="1"
                    HorizontalAlignment="Right"
                    Orientation="Horizontal">


                    <TextBlock
                        Margin="0,0,10,0"
                        VerticalAlignment="Center"
                        FontSize="18"
                        Foreground="{StaticResource HIK}">
                        UsersCount:
                    </TextBlock>
                    <Label
                        Name="usCount"
                        Margin="5,0,10,0"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Center"
                        FontSize="18" />


                </StackPanel>

            </Grid>

            <TextBlock
                Name="StatusWindow"
                Height="auto"
                FontSize="24" />



        </StackPanel>

        <WindowsFormsHost
            Name="axFP_CLOCKTest"
            Grid.ColumnSpan="2"
            Height="170"
            HorizontalAlignment="Stretch"
            VerticalAlignment="Top"
            Visibility="Hidden">
            <WindowsFormsHost.Child>
                <nt:AxFP_CLOCK x:Name="axFP_CLOCK" />
            </WindowsFormsHost.Child>
        </WindowsFormsHost>


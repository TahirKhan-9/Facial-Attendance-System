<UserControl
    x:Class="FaceAttendance.Forms.ucStaffMan"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:gif="http://wpfanimatedgif.codeplex.com"
    xmlns:local="clr-namespace:FaceAttendance.Forms"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:nt="clr-namespace:AxFP_CLOCKLib;assembly=AxInterop.FP_CLOCKLib"
    d:DesignHeight="685"
    d:DesignWidth="1024"
    Loaded="Udownloaded"
    mc:Ignorable="d">
    <Grid Height="auto">
        <Grid.ColumnDefinitions>
            <ColumnDefinition />
            <ColumnDefinition Width="1023*" />
        </Grid.ColumnDefinitions>
        <StackPanel
            x:Name="MainContent"
            Grid.ColumnSpan="2"
            Orientation="Vertical">
            <Grid Margin="0,2,0,5">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <TextBlock
                    Grid.Column="0"
                    FontSize="20"
                    Foreground="{StaticResource HIK}"
                    Text="Users Data Management" />
                <Button
                    x:Name="mchCh"
                    Grid.Column="0"
                    Margin="0,0,30,0"
                    Padding="20,9"
                    HorizontalAlignment="Right"
                    Click="btn_checkAll"
                    FontSize="14"
                    Style="{StaticResource 3DButtonStyle}">
                    Check All
                </Button>
                <Button
                    x:Name="grpCh"
                    Grid.Column="1"
                    Margin="0,0,0,0"
                    Padding="20,9"
                    HorizontalAlignment="Right"
                    Click="grpCh_Click"
                    FontSize="14"
                    Style="{StaticResource 3DButtonStyle}">
                    Check All
                </Button>
            </Grid>
            <!--  Div grid  for selecting devices  -->
            <Grid Height="150">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <ListView
                    x:Name="Devlist"
                    Grid.Column="0"
                    Height="150"
                    MaxHeight="170"
                    Margin="0,0,0,0"
                    HorizontalAlignment="Stretch"
                    HorizontalContentAlignment="Stretch"
                    VerticalContentAlignment="Top"
                    Background="Transparent"
                    FontSize="12"
                    Foreground="Black"
                    ItemsSource="{Binding Devices}"
                    Loaded="devIdOnload"
                    ScrollViewer.CanContentScroll="True"
                    ScrollViewer.HorizontalScrollBarVisibility="Visible"
                    ScrollViewer.VerticalScrollBarVisibility="Visible"
                    SelectionChanged="Devlist_SelectionChanged">
                    <ListBox.ItemContainerStyle>
                        <Style TargetType="{x:Type ListViewItem}">
                            <Style.Triggers>
                                <Trigger Property="IsMouseOver" Value="True">
                                    <Setter Property="Background" Value="{StaticResource HIK}" />
                                    <Setter Property="Focusable" Value="False" />

                                </Trigger>
                                <Trigger Property="IsSelected" Value="True">
                                    <Setter Property="Background" Value="{StaticResource HIK}" />
                                </Trigger>
                            </Style.Triggers>
                        </Style>

                    </ListBox.ItemContainerStyle>
                    <ListView.Resources>
                        <Style TargetType="ListViewItem">
                            <Setter Property="Height" Value="0" />

                        </Style>
                    </ListView.Resources>
                    <ListView.View>

                        <GridView>
                            <GridView.ColumnHeaderContainerStyle>
                                <Style TargetType="{x:Type GridViewColumnHeader}">
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="{x:Type GridViewColumnHeader}">
                                                <Border
                                                    Background="Transparent"
                                                    BorderBrush="Black"
                                                    BorderThickness="0,0,0,0">
                                                    <TextBlock
                                                        x:Name="ContentHeader"
                                                        Width="{TemplateBinding Width}"
                                                        Padding="0,5,5,5"
                                                        Text="{TemplateBinding Content}"
                                                        TextAlignment="Left" />
                                                </Border>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                    <Setter Property="OverridesDefaultStyle" Value="True" />
                                    <Setter Property="Foreground" Value="{StaticResource FREE}" />
                                    <Setter Property="FontFamily" Value="Champagne &amp; Limousines" />
                                    <Setter Property="FontSize" Value="12" />
                                    <Setter Property="FontWeight" Value="Bold" />
                                    <Setter Property="Background" Value="AliceBlue" />
                                    <Setter Property="BorderBrush" Value="BlanchedAlmond" />
                                    <Setter Property="BorderThickness" Value="2" />

                                </Style>
                            </GridView.ColumnHeaderContainerStyle>
                            <GridViewColumn Width="{Binding ElementName=c1, Path=ActualWidth}" Header="Device ID">
                                <GridViewColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock
                                            Padding="0"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch">
                                            <Label
                                                Padding="0"
                                                Content="{Binding MId}"
                                                FontSize="12" />
                                        </TextBlock>
                                    </DataTemplate>
                                </GridViewColumn.CellTemplate>
                            </GridViewColumn>
                            <GridViewColumn Width="{Binding ElementName=c2, Path=ActualWidth}" Header="Device IP">
                                <GridViewColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock
                                            Padding="0"
                                            HorizontalAlignment="Stretch"
                                            VerticalAlignment="Stretch">
                                            <Label
                                                Padding="0"
                                                Content="{Binding Ip}"
                                                FontSize="12" />
                                        </TextBlock>
                                    </DataTemplate>
                                </GridViewColumn.CellTemplate>
                            </GridViewColumn>
                            <GridViewColumn
                                x:Name="action"
                                Width="{Binding ElementName=c6, Path=ActualWidth}"
                                Header="Action">
                                <GridViewColumn.CellTemplate>
                                    <DataTemplate>
                                        <TextBlock
                                            Padding="0"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Stretch">
                                            <TextBlock
                                                Name="abc"
                                                Width="65"
                                                Cursor="Hand"
                                                Focusable="True">
                                                <CheckBox IsChecked="{Binding RelativeSource={RelativeSource AncestorType={x:Type ListBoxItem}}, Path=IsSelected}" />
                                                <TextBlock Margin="0,2,2,0" HorizontalAlignment="Center">
                                                    <Label
                                                        Padding="0,0,0,3"
                                                        FontFamily="Champagne &amp; Limousines"
                                                        FontSize="11"
                                                        Foreground="{StaticResource VISION}" />
                                                </TextBlock>
                                            </TextBlock>

                                        </TextBlock>

                                    </DataTemplate>
                                </GridViewColumn.CellTemplate>
                            </GridViewColumn>

                        </GridView>
                    </ListView.View>

                </ListView>

                <ListView
                    x:Name="GroupList"
                    Grid.Column="1"
                    Width="auto"
                    Height="150"
                    MaxHeight="170"

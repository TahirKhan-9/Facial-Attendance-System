            </Grid>

            <TabControl
                Height="430"
                MinHeight="350"
                Background="Transparent">

                <TabItem
                    Height="30"
                    FontSize="16"
                    Foreground="{StaticResource HIK}"
                    Header="Download Users Info"
                    Loaded="TabItem_Loaded">

                    <StackPanel Orientation="Vertical">

                        <Grid Height="480" Margin="0,0,0,0">

                            <StackPanel>
                                <Grid
                                    Grid.Row="0"
                                    Height="50"
                                    Margin="0,0,0,0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="3.5*" />
                                        <ColumnDefinition Width="0.5*" />
                                        <ColumnDefinition Width="1*" />

                                    </Grid.ColumnDefinitions>

                                    <StackPanel Grid.Column="0" Orientation="Horizontal">
                                        <Grid
                                            Height="30"
                                            Margin="0,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center"
                                            Background="White">
                                            <!--  overlay with hint text  -->

                                            <TextBlock
                                                Name="txt_search_1"
                                                Width="200"
                                                Height="30"
                                                Padding="5,10"
                                                VerticalAlignment="Center"
                                                FontSize="11"
                                                Foreground="#777A7D"
                                                Text="Enter User ID"
                                                TextAlignment="Left" />

                                            <!--  enter term here  -->
                                            <TextBox
                                                Name="txt_search"
                                                Width="200"
                                                Height="30"
                                                Padding="5,5"
                                                Background="Transparent"
                                                GotFocus="txt_search_1_GotFocus"
                                                TextChanged="Txt_chnged_Item_resetdown_List" />

                                        </Grid>
                                        <Button
                                            Margin="0,0,25,0"
                                            Padding="20,9"
                                            HorizontalAlignment="Right"
                                            Click="SearchClick"
                                            Content="Search"
                                            FontSize="14"
                                            Style="{StaticResource 3DButtonStyle}"
                                            TabIndex="6" />
                                        <CheckBox
                                            x:Name="checkFace"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center"
                                            VerticalContentAlignment="Center">
                                            Face
                                        </CheckBox>
                                        <CheckBox
                                            x:Name="checkFing"
                                            Margin="5,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center"
                                            VerticalContentAlignment="Center">
                                            Finger
                                        </CheckBox>
                                        <CheckBox
                                            x:Name="checkCard"
                                            Margin="5,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center"
                                            VerticalContentAlignment="Center">
                                            Card
                                        </CheckBox>
                                        <Button
                                            Margin="5,0"
                                            Padding="20,9"
                                            HorizontalAlignment="Left"
                                            Click="btnDownUser"
                                            Content="Update"
                                            FontSize="14"
                                            Style="{StaticResource 3DButtonStyle}"
                                            TabIndex="6" />
                                    </StackPanel>
                                    <!--<TextBox
                                        x:Name="txt_search"
                                        Grid.Column="0"
                                        Grid.ColumnSpan="2"
                                        Width="200"
                                        Height="30"
                                        Margin="0,8,0,7"
                                        HorizontalAlignment="Left"
                                        FontSize="15"
                                        Foreground="Black"
                                        Text=""
                                        TextChanged="Txt_chnged_ItemSo_reset" />-->
                                    <StackPanel
                                        Grid.Column="2"
                                        HorizontalAlignment="Right"
